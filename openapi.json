                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GetChatScoreResponse": {
                "type": "object",
                "properties": {
                    "cyber_score_score": {
