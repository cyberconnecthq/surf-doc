    "paths": {
        "/enterprise-chat/ask/{ticker}": {
            "get": {
                "description": "Returns a quick recent price movement analysis of a selected token.\nToken ticker (path parameter) is required.",
                "tags": [
                    "Enterprise Chat"
                ],
                "summary": "Quick-Select",
                "parameters": [
                    {
                        "description": "Ticker symbol",
                        "name": "ticker",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns quick ask response on success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.GenericResponse-model_AskResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "When ticker or template_id is missing",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    }
                },
                "x-public-api": true
            }
        },
        "/enterprise-chat/events": {
            "get": {
                "description": "Provides a forward-looking summary of significant scheduled economic or financial events expected to occur during the evening (based on Korean Standard Time) of a selected date.",
