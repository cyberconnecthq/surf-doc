    "servers": [
        {
            "url": "https://api.asksurf.ai/surf-ai"
        }
    ],
    "components": {
        "securitySchemes": {
            "AccessToken": {
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        },
        "schemas": {
            "model.AskResponse": {
                "type": "object",
                "properties": {
                    "content": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    }
                }
            },
            "model.BaseResponse": {
                "type": "object",
                "properties": {
                    "error_code": {
                        "type": "string",
                        "example": "FORBIDDEN"
                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.CompletionsChoice": {
                "type": "object",
                "properties": {
                    "finish_reason": {
                        "description": "Indicates why the model stopped generating. Common values:\n- stop — normal completion\n- length — hit max token limit\n- tool_calls — model invoked a tool\n- content_filter — blocked by safety filters\n- error — unexpected interruption",
                        "type": "string",
                        "example": "stop"
                    },
                    "index": {
                        "description": "The index of the choice",
                        "type": "integer",
                        "example": 0
                    },
                    "message": {
                        "$ref": "#/components/schemas/model.CompletionsMessage"
                    }
                }
            },
            "model.CompletionsMessage": {
                "type": "object",
                "properties": {
                    "content": {
                        "type": "string",
                        "example": "Here is a summary of today's Bitcoin market..."
                    },
                    "reasoning": {
                        "description": "A field to showcase model's thinking process.",
                        "type": "string",
                        "example": "Internal chain-of-thought or brief rationale"
                    },
                    "role": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/model.Role"
                            }
                        ],
                        "example": "assistant"
                    }
                }
            },
            "model.CompletionsProxyResponse": {
                "type": "object",
                "properties": {
                    "choices": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/model.CompletionsChoice"
                        }
                    },
                    "created": {
                        "description": "Timestamp indicating when the message was generated.",
                        "type": "integer",
                        "example": 1764312947
                    },
                    "id": {
                        "description": "A unique identifier for the generated message. Used for internal tracking.",
                        "type": "string",
                        "example": "chatcmpl-1764312947577"
                    },
                    "model": {
                        "type": "string",
                        "example": "surf-ask"
                    },
                    "object": {
                        "type": "string",
                        "example": "chat.completion"
                    },
                    "usage": {
                        "$ref": "#/components/schemas/model.CompletionsUsage"
                    }
                }
            },
            "model.CompletionsRequest": {
                "type": "object",
                "properties": {
                    "messages": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/model.Message"
                        }
                    },
                    "model": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/model.LLMModel"
                            }
                        ],
                        "example": "surf-ask"
                    },
                    "stream": {
                        "type": "boolean",
                        "example": false
                    }
                }
            },
            "model.CompletionsUsage": {
                "type": "object",
                "properties": {
                    "completion_tokens": {
                        "type": "integer",
                        "example": 113
                    },
                    "prompt_tokens": {
                        "type": "integer",
                        "example": 13026
                    },
                    "total_tokens": {
                        "type": "integer",
                        "example": 13760
                    }
                }
            },
            "model.GenericResponse-model_AskResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/model.AskResponse"
                    },
                    "error_code": {
                        "type": "string",
                        "example": "FORBIDDEN"
                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GenericResponse-model_GetChatScoreResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/model.GetChatScoreResponse"
                    },
                    "error_code": {
                        "type": "string",
                        "example": "FORBIDDEN"
                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GenericResponse-model_QuestionResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/model.QuestionResponse"
                    },
                    "error_code": {
                        "type": "string",
                        "example": "FORBIDDEN"
                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GenericResponse-model_ResearchResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/model.ResearchResponse"
                    },
                    "error_code": {
                        "type": "string",
                        "example": "FORBIDDEN"
                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GenericResponse-model_SentimentResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/model.SentimentResponse"
                    },
                    "error_code": {
                        "type": "string",
                        "example": "FORBIDDEN"
                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GetChatScoreResponse": {
                "type": "object",
                "properties": {
                    "cyber_score_score": {
                        "type": "number"
                    },
                    "mind_share_score": {
                        "type": "number"
                    },
                    "smart_followers_count_score": {
                        "type": "number"
                    },
                    "weighted_score": {
                        "type": "number"
                    }
                }
            },
            "model.LLMModel": {
                "type": "string",
                "enum": [
                    "surf-ask",
                    "surf-research"
                ],
                "x-enum-varnames": [
                    "LLMModelSURFASK",
                    "LLMModelSURFRESEARCH"
                ]
            },
            "model.Message": {
                "type": "object",
                "properties": {
                    "content": {
                        "description": "The final user-visible text generated by the assistant.",
                        "type": "string",
                        "example": "Summarize today's Bitcoin market."
                    },
                    "role": {
                        "description": "The role of the message. The source of the message. Include:\n- system — system instructions\n- user — user input\n- assistant — model output\n- tool — tool responses",
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/model.Role"
                            }
                        ],
                        "example": "user"
                    }
                }
            },
            "model.QuestionResponse": {
                "type": "object",
                "properties": {
                    "content": {
                        "type": "string"
                    }
                }
            },
            "model.ResearchResponse": {
                "type": "object",
                "properties": {
                    "content": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    }
                }
            },
            "model.Role": {
                "type": "string",
                "enum": [
                    "user",
                    "assistant",
                    "system"
                ],
                "x-enum-varnames": [
                    "RoleUSER",
                    "RoleASSISTANT",
                    "RoleSYSTEM"
                ]
            },
            "model.SentimentResponse": {
                "type": "object",
                "properties": {
                    "content": {
                        "type": "string"
                    },
                    "fall_token_count": {
                        "type": "integer"
                    },
                    "fear_greed_index": {
                        "type": "number"
                    },
                    "neutral_token_count": {
                        "type": "number"
                    },
                    "rise_token_count": {
                        "type": "integer"
                    }
                }
