                    },
                    "message": {
                        "type": "string",
                        "example": "success"
                    },
                    "success": {
                        "type": "boolean",
                        "example": true
                    }
                }
            },
            "model.GenericResponse-model_ResearchResponse": {
                "type": "object",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/model.ResearchResponse"
                    },
                    "error_code": {
                        "type": "string",
