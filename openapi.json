    "paths": {
        "/enterprise-chat/ask/{ticker}": {
            "get": {
                "description": "Returns quick ask response content for a given ticker including title and answer content\nBoth ticker (path parameter) and template_id (query parameter) are required",
                "tags": [
                    "Enterprise Chat"
                ],
                "summary": "Quick-Select",
                "parameters": [
                    {
                        "description": "Ticker symbol",
                        "name": "ticker",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "Template ID",
                        "name": "template_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns quick ask response on success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.GenericResponse-model_AskResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "When ticker or template_id is missing",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    }
                },
                "x-public-api": true
            }
        },
        "/enterprise-chat/research/{ticker}": {
            "get": {
                "description": "Returns research content for a given ticker including title and detailed content\nBoth ticker (path parameter) and template_id (query parameter) are required",
                "tags": [
                    "Enterprise Chat"
                ],
                "summary": "Token Research",
                "parameters": [
                    {
                        "description": "Ticker symbol",
                        "name": "ticker",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "Template ID",
                        "name": "template_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns research information on success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.GenericResponse-model_ResearchResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "When ticker or template_id is missing",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    }
                },
                "x-public-api": true
            }
        },
        "/enterprise-chat/score/{ticker}": {
            "get": {
                "description": "Returns various scores for a given ticker including average score, smart follower count score, mind share score, and cyber score\nBoth ticker (path parameter) and template_id (query parameter) are required",
                "tags": [
                    "Enterprise Chat"
                ],
                "summary": "Token Scoring",
                "parameters": [
                    {
                        "description": "Ticker symbol",
                        "name": "ticker",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "description": "Template ID",
                        "name": "template_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns chat score information on success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.GenericResponse-model_GetChatScoreResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "When ticker or template_id is missing",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    }
                },
                "x-public-api": true
            }
        }
    },
    "servers": [
        {
            "url": "https://api.asksurf.ai/huginn/v2"
        }
    ],
    "components": {
        "schemas": {
            "model.AskResponse": {
                "type": "object",
                "properties": {
                    "content": {
                        "type": "string"
