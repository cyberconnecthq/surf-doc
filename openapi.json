        "/v1/chat/completions": {
            "post": {
                "security": [
                    {
                        "AccessToken": []
                    }
                ],
                "description": "OpenAI-compatible Chat Completions. Use Bearer authorization. When stream=true, responds as Server-Sent Events (SSE); otherwise returns JSON.",
                "tags": [
                    "completions"
                ],
                "summary": "Chat Completions",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/model.CompletionsRequest"
                            }
                        }
                    },
                    "description": "Request body (OpenAI-compatible)",
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Returns JSON when stream=false and SSE chunks when stream=true.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.CompletionsProxyResponse"
                                },
                                "examples": {
                                    "json": {
                                        "summary": "Non-streaming JSON response",
                                        "value": {
                                            "id": "chatcmpl-abc123",
                                            "object": "chat.completion",
                                            "created": 1699890366,
                                            "model": "surf-ask",
                                            "choices": [
                                                {
                                                    "index": 0,
                                                    "finish_reason": "stop",
                                                    "message": {
                                                        "role": "assistant",
                                                        "content": "BTC is trading flat in the last 24h with rising open interest."
                                                    }
                                                }
                                            ],
                                            "usage": {
                                                "prompt_tokens": 23,
                                                "completion_tokens": 12,
                                                "total_tokens": 35
                                            }
                                        }
                                    },
                                    "sse": {
                                        "summary": "Streaming SSE chunks (text/event-stream)",
                                        "description": "Rendered in the doc tab to illustrate stream=true output.",
                                        "value": "data: {\\\"id\\\":\\\"chatcmpl-abc123\\\",\\\"object\\\":\\\"chat.completion.chunk\\\",\\\"created\\\":1699890366,\\\"model\\\":\\\"surf-ask\\\",\\\"choices\\\":[{\\\"index\\\":0,\\\"delta\\\":{\\\"role\\\":\\\"assistant\\\",\\\"content\\\":\\\"Hello\\\"},\\\"finish_reason\\\":null}]}\\n\\ndata: {\\\"id\\\":\\\"chatcmpl-abc123\\\",\\\"object\\\":\\\"chat.completion.chunk\\\",\\\"created\\\":1699890366,\\\"model\\\":\\\"surf-ask\\\",\\\"choices\\\":[{\\\"index\\\":0,\\\"delta\\\":{\\\"content\\\":\\\" world\\\"},\\\"finish_reason\\\":null}]}\\n\\ndata: [DONE]\\n\\n"
                                    }
                                }
                            },
                            "text/event-stream": {
                                "schema": {
                                    "type": "string"
                                },
                                "examples": {
                                    "sse": {
                                        "summary": "Streaming SSE response",
                                        "value": "data: {\\\"id\\\":\\\"chatcmpl-abc123\\\",\\\"object\\\":\\\"chat.completion.chunk\\\",\\\"created\\\":1699890366,\\\"model\\\":\\\"surf-ask\\\",\\\"choices\\\":[{\\\"index\\\":0,\\\"delta\\\":{\\\"role\\\":\\\"assistant\\\",\\\"content\\\":\\\"Hello\\\"},\\\"finish_reason\\\":null}]}\\n\\ndata: {\\\"id\\\":\\\"chatcmpl-abc123\\\",\\\"object\\\":\\\"chat.completion.chunk\\\",\\\"created\\\":1699890366,\\\"model\\\":\\\"surf-ask\\\",\\\"choices\\\":[{\\\"index\\\":0,\\\"delta\\\":{\\\"content\\\":\\\" world\\\"},\\\"finish_reason\\\":null}]}\\n\\ndata: [DONE]\\n\\n"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            },
                            "text/event-stream": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            },
                            "text/event-stream": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    },
                    "502": {
                        "description": "Bad Gateway",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            },
                            "text/event-stream": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/market/events": {
            "get": {
                "security": [
                    {
                        "AccessToken": []
                    }
                ],
                "description": "Provides a forward-looking summary of significant scheduled economic or financial events expected to occur during the evening (based on Korean Standard Time) of a selected date.",
                "tags": [
                    "Market"
                ],
                "summary": "Major Events Tonight",
                "parameters": [
                    {
                        "description": "Date in YYYY-MM-DD format. If date is not provided, returns the most recent data",
                        "name": "date",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns major events on success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.GenericResponse-model_QuestionResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "When date format is invalid",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "When message not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/model.BaseResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
